{
  "name": "seiai",
  "version": "1.0.0",
  "description": "Claude Code-style CLI for generating Sei AI dApps with natural language",
  "main": "dist/index.js",
  "bin": {
    "seiai": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && cp -r ./templates ./dist/templates && cp -r ./examples ./dist/examples",
    "dev": "node --loader ts-node/esm src/index.ts",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sei",
    "blockchain",
    "ai",
    "cli",
    "dapp",
    "generator",
    "natural-language"
  ],
  "author": "Sei Agent Studio",
  "license": "MIT",
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^9.2.15",
    "ora": "^8.0.1",
    "figlet": "^1.7.0",
    "gradient-string": "^2.0.2",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/boxen": "^3.0.1",
    "@types/inquirer": "^9.0.7",
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.5",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sei-agent-studio/sei-agent-studio.git",
    "directory": "packages/seiai-cli"
  }
}