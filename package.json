{
  "name": "sei-agent-studio",
  "version": "1.0.0",
  "description": "A comprehensive agent framework for Sei blockchain with natural language dApp generation",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*",
    "templates/*"
  ],
  "scripts": {
    "build": "tsc -b",
    "test": "pnpm -r test",
    "dev": "pnpm -r dev",
    "clean": "pnpm -r clean && rm -rf node_modules && rm -rf packages/*/dist",
    "typecheck": "tsc -b --noEmit",
    "lint": "pnpm -r lint",
    "publish-packages": "pnpm -r publish --access public"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}